    {
      "name": "c7-button",
      "description": "按钮组件，基于Element Plus的el-button封装，支持多种预设类型和异步操作。",
      "version": "c7-plus >=1.0.0",
      "example": "<c7-button type=\"primary\" label=\"确定\" @click=\"handleClick\"></c7-button>",
      "docUrl": "docs/components/c7/button.md",
      "props": [
        {
          "name": "type",
          "type": "string",
          "description": "按钮类型",
          "defaultValue": "primary",
          "required": false
        },
        {
          "name": "btnType",
          "type": "string",
          "description": "预设按钮类型(add/edit/delete/query/refresh/upload/download/submit/cancel)",
          "defaultValue": "",
          "required": false
        },
        {
          "name": "plain",
          "type": "boolean",
          "description": "是否为朴素按钮",
          "defaultValue": true,
          "required": false
        },
        {
          "name": "label",
          "type": "string",
          "description": "按钮内容",
          "defaultValue": "",
          "required": false
        },
        {
          "name": "clickFunction",
          "type": "function",
          "description": "点击执行的异步函数",
          "defaultValue": null,
          "required": false
        },
        {
          "name": "confirm",
          "type": "boolean",
          "description": "是否弹出确认框",
          "defaultValue": false,
          "required": false
        },
        {
          "name": "confirmMessage",
          "type": "string",
          "description": "确认框提示内容",
          "defaultValue": "",
          "required": false
        },
        {
          "name": "isSuccessCallback",
          "type": "boolean",
          "description": "是否开启成功之后回调",
          "defaultValue": false,
          "required": false
        },
        {
          "name": "successMessage",
          "type": "string",
          "description": "成功之后提示信息",
          "defaultValue": "",
          "required": false
        },
        {
          "name": "isErrorCallback",
          "type": "boolean",
          "description": "是否开启失败回调",
          "defaultValue": false,
          "required": false
        },
        {
          "name": "checkSuccess",
          "type": "function",
          "description": "检查接口调用是否成功",
          "defaultValue": "(res) => res.code === 200",
          "required": false
        },
        {
          "name": "errorMessageType",
          "type": "string",
          "description": "错误提示类型(msg/res)",
          "defaultValue": "res",
          "required": false
        },
        {
          "name": "errorMessage",
          "type": "string",
          "description": "错误提示信息",
          "defaultValue": "msg",
          "required": false
        },
        {
          "name": "validate",
          "type": "boolean",
          "description": "是否进行参数校验",
          "defaultValue": false,
          "required": false
        },
        {
          "name": "validateRef",
          "type": "object",
          "description": "参数校验的ref",
          "defaultValue": null,
          "required": false
        }
      ],
      "events": [
        {
          "name": "successCallback",
          "description": "成功回调事件",
          "parameters": "result"
        },
        {
          "name": "errorCallback",
          "description": "错误回调事件",
          "parameters": "result, message"
        }
      ],
      "slots": [
        {
          "name": "default",
          "description": "按钮内容插槽"
        }
      ]
    },
